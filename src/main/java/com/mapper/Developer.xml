<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//Mybatis.org/DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.mybatis.spring">
    <select id="getDeveloper" resultType="com.bean.Developer_administrator" parameterType="int">
        SELECT developer_id from developer_administrator where developer_id = #{id} and password = #{}
    </select>
</mapper>